srd3 <- read.table("./data_sr3", header=T, sep=",")
max_delay <- max(srd3$avg_delay)
plot(x=srd3$p, y=srd3$avg_delay, ylim=c(min(srd3$avg_delay),1.1*max(srd3$avg_delay)), col='blue', type="o", main='Task 3 - Selective Repeat ARQ', ylab='Delay (Seconds)', xlab='Packet Loss Probability P')
abline(lm(srd3$avg_delay~srd3$p))
box()

